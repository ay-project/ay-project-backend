    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}\\server_services\\main_service\\server.js"
        },
        {
        "type": "node",
        "request": "launch",
        "name": "Mocha All",
        "program": "${workspaceFolder}\\server_services\\main_service\\node_modules\\mocha\\bin\\_mocha",
        "args": [
            "--timeout",
            "999999",
            "--colors",
            "${workspaceFolder}\\server_services\\main_service\\test"
        ],
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
    },
    {
        "type": "node",
        "request": "launch",
        "name": "Mocha Current File",
        "program": "${workspaceFolder}\\server_services\\main_service\\node_modules\\mocha\\bin\\_mocha",
        "args": [
            "--timeout",
            "999999",
            "--colors",
            "${file}"
        ],
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
    }
    ]
}